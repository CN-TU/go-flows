{
    "exporters": {
        "csv": {
            "type": "ipfix",
            "options": {
                "out": "out.ipfix",
                "spec": "test.iespec"
            }
        }
    },
    "features": [
        {
            "exporters": [
                "csv"
            ],
            "input": [
                {
                    "features": [
                        "sourceIPAddress",
                        "destinationIPAddress",
                        "protocolIdentifier",
                        "sourceTransportPort",
                        "destinationTransportPort",
                        {"mean": ["octetTotalCount"]},
                        "flowEndReason",
                        "flowEndNanoseconds",
                        "ipTotalLength",
                        {"apply": ["ipTotalLength", {"select": [{"less": ["ipTotalLength", 80]}]}]},
                        "minimumIpTotalLength",
                        "maximumIpTotalLength"
                    ],
                    "bidirectional": true,
                    "key_features": [
                        "sourceIPAddress",
                        "destinationIPAddress",
                        "protocolIdentifier",
                        "sourceTransportPort",
                        "destinationTransportPort"
                    ]
                }
            ]
        }
    ]
}
