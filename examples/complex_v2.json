{
    "version": "v2",
        "preprocessing": {
            "flows": [
            {
                "features": [
                    "sourceIPAddress",
                    "destinationIPAddress",
                    "protocolIdentifier",
                    "sourceTransportPort",
                    "destinationTransportPort",
                    {"mean": ["octetTotalCount"]},
                    "flowEndReason",
                    "flowEndNanoseconds",
                    "ipTotalLength",
                    {"apply": ["ipTotalLength", {"select": [{"less": ["ipTotalLength", 80]}]}]},
                    "minimumIpTotalLength",
                    "maximumIpTotalLength"
                ],
                "bidirectional": true,
                "key_features": [
                    "sourceIPAddress",
                    "destinationIPAddress",
                    "protocolIdentifier",
                    "sourceTransportPort",
                    "destinationTransportPort"
                ]
            }
            ]
        }
}
